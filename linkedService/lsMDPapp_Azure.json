{
	"name": "lsMDPapp_Azure",
	"properties": {
		"description": "Connection between ADF and processing framework metadata SQLDB.",
		"parameters": {
			"server": {
				"type": "string",
				"defaultValue": "sndffcsq4401.database.windows.net"
			},
			"dbname": {
				"type": "string",
				"defaultValue": "sndffcdb4401"
			}
		},
		"annotations": [],
		"type": "AzureSqlDatabase",
		"typeProperties": {
			"connectionString": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=@{linkedService().server};Initial Catalog=@{linkedService().dbname}"
		},
		"connectVia": {
			"referenceName": "AutoResolveIntegrationRuntime",
			"type": "IntegrationRuntimeReference"
		}
	},
	"type": "Microsoft.Synapse/workspaces/linkedservices"
}