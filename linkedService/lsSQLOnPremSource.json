{
	"name": "lsSQLOnPremSource",
	"properties": {
		"parameters": {
			"ServerName": {
				"type": "string"
			},
			"DBName": {
				"type": "string"
			},
			"UserName": {
				"type": "string"
			},
			"secret": {
				"type": "string"
			},
			"keyvault": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "SqlServer",
		"typeProperties": {
			"connectionString": "Integrated Security=False;Data Source=@{linkedService().ServerName};Initial Catalog=@{linkedService().DBName};User ID=@{linkedService().UserName}",
			"password": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "Keys",
					"type": "LinkedServiceReference",
					"parameters": {
						"keyvaultdnsname": {
							"value": "@linkedService().keyvault",
							"type": "Expression"
						}
					}
				},
				"secretName": {
					"value": "@linkedService().secret",
					"type": "Expression"
				}
			}
		},
		"connectVia": {
			"referenceName": "SHIR",
			"type": "IntegrationRuntimeReference"
		}
	},
	"type": "Microsoft.Synapse/workspaces/linkedservices"
}