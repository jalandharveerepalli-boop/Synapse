{
	"name": "PSIntegration",
	"properties": {
		"parameters": {
			"Host": {
				"type": "string"
			},
			"ServiceName": {
				"type": "string"
			},
			"Port": {
				"type": "string"
			},
			"UserName": {
				"type": "string"
			},
			"SecretName": {
				"type": "string"
			},
			"keyvault": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Oracle",
		"typeProperties": {
			"connectionString": "host=@{linkedService().Host};port=@{linkedService().Port};sid=@{linkedService().ServiceName};user id=@{linkedService().UserName}",
			"password": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "Keys",
					"type": "LinkedServiceReference",
					"parameters": {
						"keyvaultdnsname": {
							"value": "@linkedService().keyvault",
							"type": "Expression"
						}
					}
				},
				"secretName": {
					"value": "@linkedService().SecretName",
					"type": "Expression"
				}
			}
		},
		"connectVia": {
			"referenceName": "AutoResolveIntegrationRuntime",
			"type": "IntegrationRuntimeReference"
		}
	},
	"type": "Microsoft.Synapse/workspaces/linkedservices"
}