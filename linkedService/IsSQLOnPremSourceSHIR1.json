{
	"name": "IsSQLOnPremSourceSHIR1",
	"properties": {
		"parameters": {
			"ServerName": {
				"type": "string"
			},
			"DatabaseName": {
				"type": "string"
			},
			"User": {
				"type": "string"
			},
			"Secret": {
				"type": "string"
			},
			"keyvaultdns": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "SqlServer",
		"typeProperties": {
			"connectionString": "Integrated Security=False;Data Source=@{linkedService().ServerName};Initial Catalog=@{linkedService().DatabaseName};User ID=@{linkedService().User}",
			"password": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "Keys",
					"type": "LinkedServiceReference",
					"parameters": {
						"keyvaultdnsname": {
							"value": "@linkedService().keyvaultdns",
							"type": "Expression"
						}
					}
				},
				"secretName": {
					"value": "@linkedService().Secret",
					"type": "Expression"
				}
			}
		}
	}
}